[libs]
flow-typed
[ignore]
.*/kefir/.*
.*react-simplemde-editor.*
[options]
module.name_mapper='^tests' -> '<PROJECT_ROOT>/src/tests'
module.name_mapper='^@client' -> '<PROJECT_ROOT>/src/@client'
module.name_mapper='^ui-kit' -> '<PROJECT_ROOT>/src/ui-kit'
module.name_mapper='^components' -> '<PROJECT_ROOT>/src/components'
module.name_mapper='^panels' -> '<PROJECT_ROOT>/src/components/panels'
module.name_mapper='^.*\.pcss$' -> 'empty/object'
esproposal.class_static_fields=enable
esproposal.class_instance_fields=enable
module.file_ext=.js
module.file_ext=.json
module.file_ext=.jsx
module.file_ext=.css
module.file_ext=.scss
module.file_ext=.pcss
