  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build:webpack": "NODE_ENV=production node -r dotenv/config node_modules/.bin/webpack --config webpack.config.prod.js -p",
    "build": "npm run clean && npm run build:webpack",
    "start-dev": "node -r dotenv/config devServer.js",
    "start-debug-production": "API_CONNECTION=https://beaprogrammer.org/api node -r dotenv/config devServer.js",
    "start-server": "node -r dotenv/config server.js",
    "start": "npm run build && npm run start-server",
    "lint": "eslint src",
    "pretty": "prettier --write 'src/**/*.js'",
    "flow": "flow",
    "flowtyped": "flow-typed",
    "test": "npm run lint && npm run flow && npm run jest",
    "jest": "jest",
    "precommit": "lint-staged",
    "yo": "yo",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "author": "",
  "license": "ISC",
