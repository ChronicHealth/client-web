  "lint-staged": {
    "{src,pages}/**/*.{js,json,css}": [
      "prettier --write '**/*.js'",
      "git add"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "^.*[.](pcss|css|sass|less)$": "<rootDir>/src/tests/EmptyModule"
    },
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/src/tests/"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "setupFiles": [
      "jest-localstorage-mock"
    ]
  }
